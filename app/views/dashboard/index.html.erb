<div class="container mt-4">
  <h1 class="mb-4">User Subscription Dashboard</h1>

  <div class="row">
    <div class="col-md-6">
      <table class="table table-striped">
        <thead>
        <tr>
          <th>Name</th>
          <th>Age</th>
          <th>Item Details</th>
          <th>Subscription</th>
        </tr>
        </thead>
        <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.age %></td>
            <td><%= user.age %></td>
            <td><%= user.subscription.name %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>

    <div class="col-md-6">
<!--      <canvas id="subscriptionChart" width="400" height="400"></canvas>-->
    </div>
  </div>
</div>


